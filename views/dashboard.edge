<!DOCTYPE html>
<html lang="en">


<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>WEPAYS : Home</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <!-- ================= Favicon ================== -->
    <!-- Standard -->
    <link rel="shortcut icon" href="https://placehold.it/64.png/000/fff">
    <!-- Retina iPad Touch Icon-->
    <link rel="apple-touch-icon" sizes="144x144" href="https://placehold.it/144.png/000/fff">
    <!-- Retina iPhone Touch Icon-->
    <link rel="apple-touch-icon" sizes="114x114" href="https://placehold.it/114.png/000/fff">
    <!-- Standard iPad Touch Icon-->
    <link rel="apple-touch-icon" sizes="72x72" href="https://placehold.it/72.png/000/fff">
    <!-- Standard iPhone Touch Icon-->
    <link rel="apple-touch-icon" sizes="57x57" href="https://placehold.it/57.png/000/fff">

    <!-- Styles -->
    <link rel="stylesheet" type="text/css" href="/assets/css/lib/weather-icons.css" />
    <link rel="stylesheet" type="text/css" href="/assets/css/lib/owl.carousel.min.css" />
    <link rel="stylesheet" type="text/css" href="/assets/css/lib/owl.theme.default.min.css" />
    <link rel="stylesheet" type="text/css" href="/assets/css/lib/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="/assets/css/lib/themify-icons.css">
    <link rel="stylesheet" type="text/css" href="/assets/css/lib/menubar/sidebar.css">
    <link rel="stylesheet" type="text/css" href="/assets/css/lib/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="/assets/css/lib/helper.css" />
    <link rel="stylesheet" type="text/css" href="/assets/css/style.css">
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script src="https://code.iconify.design/1/1.0.3/iconify.min.js"></script>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
</head>

<body onload="myFunction()">
    @if(messages.loginSucces)
    <script>
        function myFunction() {
            swal({
                title: "Welcome to WEPAYS !!",
                timer: 6000,
                showConfirmButton: false
            });
        };
    </script>
    @endif
    <div class="sidebar sidebar-hide-to-small sidebar-shrink sidebar-gestures">
        <div class="nano">
            <div class="nano-content">
                <div class="logo"><a href="#"><span style="color: rgb(25, 124, 216)">WEPAYS PAYMENT SOLUTIONS</span></a>
                </div>
                <ul>
                    <li class="label">Main</li>
                    @if(Admin)
                    <li class="active"><a href="/auth/login" class="sidebar-sub-toggle"><i class="ti-home"></i>Admin
                            Dashboard <span class="badge badge-primary"></span></a>
                        @endif
                        @if(!Admin)
                    <li class="active"><a href="/auth/login" class="sidebar-sub-toggle"><i class="ti-home"></i>
                            Dashboard
                            <span class="badge badge-primary"></span></a>
                        @endif
                    </li>

                    <li class="label">Services</li>
                    <li class=""><a href="/topUp"><i class="ti-money"></i> Mobile Recharge </a></li>
                    <li><a href="/dth"><i class="ti-blackboard"></i> DTH</a></li>
                    <li ><a href="/electricity"><i class="fa fa-lightbulb-o"></i>
                            Electricity</a></li>
                    <li><a href="/gas"><i class="fa fa-fire"></i>
                            Gas</a></li>
                    <li  ><a href="/water"><span class="iconify" data-icon="ion-waterdrop"
                                data-inline="false"></span>&nbsp;&nbsp;&nbsp;&nbsp; Water</a></li>
                    <li  ><a href="/broadband"><i class="material-icons">router</i>
                            Broadband</a></li>
                    <li class="label">Features</li>
                    @if(Admin)
                    <li><a class="sidebar-sub-toggle"><i class="ti-face-smile"></i> Users <span
                                class="sidebar-collapse-icon ti-angle-down"></span></a>
                        <ul>
                            <li><a href="/auth/distubitors">Users Details</a></li>
                        </ul>
                    </li>
                    @endif
                    <li><a href="/auth/viewProfile"><i class="ti-user"></i> Profile</a></li>
                    <li><a href="/auth/transactions"><i class="ti-notepad"></i> Transactions</a>
                    <li><a href="/auth/contactUs"><i class="ti-file"></i>Contact Us</a></li>
                    <li><a href="/auth/logout"><i class="ti-close"></i> Logout</a></li>
                </ul>
            </div>
        </div>
    </div>
    <!-- /# sidebar -->


    <div class="header">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="float-left">
                        <div class="hamburger sidebar-toggle">
                            <span class="line"></span>
                            <span class="line"></span>
                            <span class="line"></span>
                        </div>
                    </div>
                    <div class="float-right">
                        <ul>

                            <li class="header-icon dib"><span class="user-avatar">{{users.name}} <i
                                        class="ti-angle-down f-s-10"></i></span>
                                <div class="drop-down dropdown-profile">
                                    <div class="dropdown-content-body">
                                        <ul>
                                            <li><a href="/auth/viewProfile"><i class="ti-user"></i>
                                                    <span>Profile</span></a></li>
                                            <li><a href="/auth/logout"><i class="ti-power-off"></i>
                                                    <span>Logout</span></a></li>
                                        </ul>
                                    </div>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="content-wrap">
        <div class="main">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-8 p-r-0 title-margin-right">
                        <div class="page-header">
                            <div class="page-title">
                                <h1>Hello, <span>Welcome {{users.name}}</span></h1>
                            </div>
                        </div>
                    </div>
                    <!-- /# column -->
                    <div class="col-lg-4 p-l-0 title-margin-left">
                        <div class="page-header">
                            <div class="page-title">
                                <ol class="breadcrumb">
                                    <li class="breadcrumb-item"><a href="#">Dashboard</a></li>
                                    <li class="breadcrumb-item active">Home</li>
                                </ol>
                            </div>
                        </div>
                    </div>
                    <!-- /# column -->
                </div>
                <section id="main-content">
                    <div class="row">
                        <div class="col-lg-12">
                            <h1>User Name : <span style="text-transform: uppercase;">{{users.name}}</span></h1>
                            <h2>Unique Id : {{users.uniqueId}}</h2>
                        </div>
                        <div class="col-lg-3">
                            <div class="card">
                                <div class="stat-widget-one">
                                    <div class="stat-icon dib"><i class="fa fa-user-circle-o"
                                            style="color:rgb(25, 124, 216)"></i>
                                    </div>
                                    <div class="stat-content dib">
                                        <div class="stat-text">Last Login Date</div>
                                        <div class="stat-digit">{{users.lastlogin.toDateString()}} </div>
                                        {{users.lastlogin.toLocaleTimeString()}}
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- <div class="col-lg-3">
                            <div class="card">
                                <div class="stat-widget-one">
                                    <div class="stat-icon dib"><i class="fa fa-rupee"
                                            style="color:rgb(25, 216, 25)"></i>
                                    </div>
                                    <div class="stat-content dib">
                                        <div class="stat-text">Wallet Balance</div>
                                        <div class="stat-digit"></div>
                                    </div>
                                </div>
                            </div>
                        </div> -->


                        <!-- /# column -->
                        <!-- /# row -->


                        <!-- /# column -->

                    </div>
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="footer">
                                <p>2019 © WEPAYS USER Board. - Developed by Alpha Technologies <a href="#"></a></p>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>
    <div id="search">
        <button type="button" class="close">×</button>
        <form>
            <input type="search" value="" placeholder="type keyword(s) here" />
            <button type="submit" class="btn btn-primary">Search</button>
        </form>
    </div>
    <!-- jquery vendor -->
