  <!-- Modal -->
  <div class="modal fade" id="AdminModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle"
    aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLongTitle">Update User Details</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="col-lg-12">
          <form action="/AdmiUusers/update/{{users._id}}" method="POST" encType="multipart/form-data">
            <div class="row">
              <div class="form-group">
                <label>Member Name</label>
                <input type="text" value="{{users.name}}" name="name" required class="form-control"
                  placeholder="User Name">
              </div>
              <div class="form-group">
                <label>Unique ID</label>
                <input type="text" value="{{users.uniqueId}}" name="uniqueId" required class="form-control"
                  placeholder="Unique ID">
              </div>
              <div class="form-group">
                <label>Email address</label>
                <input type="email" value="{{users.email}}" name="email" required class="form-control"
                  placeholder="Email">
              </div>
              <div class="form-group">
                <label>Password</label>
                <input type="password" name="password" required class="form-control" placeholder="password">
              </div>
              <div class="form-group">
                <label>Mobile Number</label>
                <input type="text" value="{{users.number}}" name="number" required class="form-control"
                  placeholder="Mobile Number">
              </div>
              <div class="form-group">
                <label>Pan Number</label>
                <input type="text" value="{{users.pan}}" name="pan" class="form-control" placeholder="Pan Number">
              </div>
              <div class="form-group">
                <label>Shop Name</label>
                <input type="text" value="{{users.shopname}}" name="shopname" required class="form-control"
                  placeholder="Shop Name">
              </div>

              <div class="form-group">
                <label>Pin Code</label>
                <input type="text" value="{{users.pincode}}" name="pincode" class="form-control" placeholder="Pin Code">
              </div>

              <div class="form-group">
                <input type="text" value="{{users.role}}" name="role" class="form-control" hidden>
              </div>
              <div class="form-group">
                <label>Contact Address</label>
                <textarea value="{{users.contactaddress}}" name="contactaddress" placeholder="Address"
                  class="form-control">{{users.contactaddress}}</textarea>
              </div>
              <div class="form-group">
                <label>Shop Address</label>
                <textarea value="{{users.shopaddress}}" name="shopaddress" placeholder="Address"
                  class="form-control">{{users.shopaddress}}</textarea>
              </div>
              <div class="form-group">
                <input type="text" value="{{users.KYCStatus}}" name="KYCStatus" class="form-control" value="user">
              </div>
              <div class="form-group">
                <input type="text" value="{{users.memberstatus}}" name="memberstatus" class="form-control" value="user">
              </div>
            </div>
            <center> <button type="submit" class="btn btn-primary btn-flat m-b-30 m-t-30">Update</button>
            </center>
          </form>
        </div>
      </div>
    </div>
  </div>





